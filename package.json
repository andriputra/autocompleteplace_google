{
  "name": "autocompleteplace_google",
  "version": "1.0.0",
  "description": "A ReactJS for show map with draggable and type search place",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:staging": "env-cmd -f .env.staging npm run build"
  },
  "engines": {
    "node": "10.10.0"
  },
  "author": "Agus Andri Putra",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "mongoose": "^5.7.5",
    "path": "^0.12.7",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "validator": "^10.8.0"
  }
}
